{
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "types": ["node", "react/experimental", "react-dom/experimental"],
    "jsx": "react",
    "allowImportingTsExtensions": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
