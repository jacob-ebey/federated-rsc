{
  "include": ["./entry/**/*", "./lib/**/*"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": ["DOM", "ES2022"],
    "types": ["node", "react/experimental", "react-dom/experimental"],
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true
  }
}
